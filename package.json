{
  "name": "django-gui",
  "displayName": "Django GUI",
  "description": "A GUI for executing Django commands.",
  "version": "0.0.1",
  "publisher": "Sasidhar Akurathi",
  "engines": {
    "vscode": "^1.104.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./extension.js",
  "contributes": {
    "commands": [{
      "command": "django-gui.start",
      "title": "Django: Open GUI Command Panel",
      "category": "Django"
    }],
    
  "viewsContainers": {
      "activitybar": [
        {
          "id": "django-gui-container",
          "title": "Django GUI",
          "icon": "media/django-icon.svg"
        }
      ]
    },
    "views": {
      "django-gui-container": [
        {
          "id": "django-commands-view",
          "name": "Commands"
        }
      ]
    }
  },
  "scripts": {
    "test": "node ./test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.104.0",
    "@types/mocha": "^10.0.10",
    "@types/node": "22.x",
    "eslint": "^9.34.0",
    "@vscode/test-cli": "^0.0.11",
    "@vscode/test-electron": "^2.5.2",
    "glob": "^8.0.3",
    "typescript": "^4.9.3"
  },
  "extensionDependencies": [
    "ms-python.python"
  ]
}
